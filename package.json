{
  "name": "babel-plugin-inline-classnames",
  "version": "2.0.1",
  "description": "Babel plugin which inlines the result of classnames",
  "keywords": [
    "babel-plugin",
    "classnames",
    "inline",
    "css",
    "css-modules",
    "style",
    "styles"
  ],
  "homepage": "https://github.com/avaly/babel-plugin-inline-classnames",
  "bugs": {
    "url": "https://github.com/avaly/babel-plugin-inline-classnames/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Valentin Agachi",
    "url": "http://agachi.name/"
  },
  "files": [
    "dist/index.js"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/avaly/babel-plugin-inline-classnames.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "commitmsg": "validate-commit-msg",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "prepublish": "yarn build",
    "pretty": "prettier --use-tabs --single-quote --trailing-comma all --write \"**/*.js\"",
    "release": "standard-version",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "classnames": "^2.2.5",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "standard-version": "^4.4.0",
    "validate-commit-msg": "^2.14.0"
  },
  "peerDependencies": {
    "@babel/core": "7.*"
  },
  "engines": {
    "node": ">=6"
  }
}
